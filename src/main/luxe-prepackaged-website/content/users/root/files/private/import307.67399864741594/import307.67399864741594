                    <j:translation_es header="Proteger a tu perro puede evitarle graves problemas de salud"
                                 j:lockTypes="_x0020_deletion_x0020_:deletion"
                                 j:locktoken="f62fd874-721b-497e-a39d-21cf5540bd01-1"
                                 jcr:language="es"
                                 jcr:lastModified="2018-06-22T18:04:54.560+01:00"
                                 jcr:lastModifiedBy="davidtobarvirbaccom"
                                 jcr:lockIsDeep="false"
