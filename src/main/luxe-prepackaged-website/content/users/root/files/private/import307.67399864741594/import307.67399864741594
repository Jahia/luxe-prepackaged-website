                    <j:translation_es header="¿Qué es y cómo puedo detectarlo?"
                                 jcr:language="es"
                                 jcr:lastModified="2018-11-28T22:19:34.642Z"
                                 jcr:lastModifiedBy="matthieurigaudvirbaccom"
                                 jcr:mixinTypes="mix:title"
