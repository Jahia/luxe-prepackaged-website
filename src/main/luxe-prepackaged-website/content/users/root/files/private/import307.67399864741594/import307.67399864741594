                                              jcr:mixinTypes="jmix:sitemap virbacmix:author"
                                              jcr:primaryType="virbacnt:advice"
                                              nbVotes="0"
                                              pictureHeader="/sites/virbac-uy/files/predefined-files/banners/Cats/chat---3-1680-x-300-.jpg"
                                              picturePreview="/sites/virbac-uy/files/Previews_x0020_312x187/gato-relajado.jpg"
                                              priority="0.5"
                                              species="/sites/virbac-uy/contents/species/gato"
