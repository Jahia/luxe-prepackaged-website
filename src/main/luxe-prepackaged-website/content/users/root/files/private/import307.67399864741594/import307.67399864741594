                    <j:translation_es header="¿Qué es y cómo puedo detectarlo?"
                                 jcr:language="es"
                                 jcr:lastModified="2018-11-28T22:17:55.368Z"
                                 jcr:lastModifiedBy="matthieurigaudvirbaccom"
                                 jcr:mixinTypes="mix:title"
                                 jcr:primaryType="jnt:translation"
                                 jcr:title="Parásitos externos en ganado bovino."
                                 pageTitle="Parásitos que cumplen su ciclo parasítico en piel o tejido subcutáneo como las garrapatas, moscas, piojos o ácaros."/>
                    <control-estrategico-de-los-paras j:originWS="default"
                                                 jcr:created="2018-11-28T22:17:50.987Z"
                                                 jcr:createdBy="matthieurigaudvirbaccom"
                                                 jcr:lastModified="2018-11-28T22:17:55.368Z"
                                                 jcr:lastModifiedBy="matthieurigaudvirbaccom"
