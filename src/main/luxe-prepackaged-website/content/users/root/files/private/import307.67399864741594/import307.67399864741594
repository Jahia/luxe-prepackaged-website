                <necesidades-alimentarias-de-los articleDate="2018-06-07T16:57:00.000+01:00"
                                             category="/sites/virbac-uy/contents/categories/articles/dermatologia"
                                             changefreq="monthly"
                                             j:originWS="default"
                                             jcr:created="2018-06-07T16:57:14.224+01:00"
                                             jcr:createdBy="arlesalvarezvirbaces"
                                             jcr:lastModified="2018-11-28T17:02:36.351Z"
                                             jcr:lastModifiedBy="matthieurigaudvirbaccom"
                                             jcr:mixinTypes="jmix:sitemap"
                                             jcr:primaryType="virbacnt:article"
                                             nbVotes="0"
