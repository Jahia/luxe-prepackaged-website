            <por-que-effitix j:lockTypes="_x0020_deletion_x0020_:deletion"
                          j:locktoken="46dfce5a-9cd4-49f0-b1f5-dc6a08e0066a-O"
                          j:originWS="default"
                          jcr:created="2018-05-18T13:37:19.600+01:00"
                          jcr:createdBy="arlesalvarezvirbaces"
                          jcr:lastModified="2018-11-29T18:35:10.220Z"
                          jcr:lastModifiedBy="matthieurigaudvirbaccom"
                          jcr:lockIsDeep="false"
                          jcr:lockOwner="matthieurigaudvirbaccom"
                          jcr:mixinTypes="jmix:markedForDeletion jmix:referencesInField jmix:vanityUrlMapped"
                          jcr:primaryType="virbacnt:freeMainContent"
