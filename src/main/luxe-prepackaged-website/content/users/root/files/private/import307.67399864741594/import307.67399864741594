                    <neumonia-fibrinosa-o-fiebre-de-e j:originWS="default"
                                                 jcr:created="2018-11-28T22:31:50.803Z"
                                                 jcr:createdBy="matthieurigaudvirbaccom"
                                                 jcr:lastModified="2018-11-28T22:33:18.770Z"
